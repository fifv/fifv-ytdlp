{
	"name": "try-ytdlp",
	"packageManager": "yarn@3.1.1",
	"version": "2.10.3",
	"main": "build/main.js",
	"author": "Fifv",
	"description": "A GUI to use yt-dlp",
	"scripts": {
		"start": "electron .",
		"watch": "webpack --mode=development --watch --devtool=inline-source-map",
		"build": "webpack --mode=production && electron-builder",
		"webpack build": "webpack --mode=production",
		"electron-forge make": "electron-forge make",
		"electron-builder": "electron-builder"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"ignore": [
					"src",
					".git",
					"playground",
					".*\\.web.*",
					"dist"
				]
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "yt-dlp-gui",
						"noMsi": true
					}
				}
			]
		}
	},
	"build": {
		"appId": "ytdlp-gui",
		"productName": "YT-dlp GUI",
		"electronVersion": "16.0.5",
		"icon": "./build/img/youtube-icon.png",
		"files": [
			"build/**",
			"package.json"
		],
		"asar": true,
		"asarUnpack": [
			"./build"
		],
		"win": {
			"target": "zip"
		}
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.63",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.63",
		"@swc/core": "^1.2.145",
		"@tippyjs/react": "^4.2.6",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^17.0.18",
		"@types/react": "^17.0.39",
		"@types/react-addons-shallow-compare": "^0.14.22",
		"@types/react-custom-scrollbars": "^4.0.10",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.12.0",
		"@typescript-eslint/parser": "^5.12.0",
		"bootstrap": "^5.1.3",
		"copy-webpack-plugin": "^10.2.4",
		"css-loader": "^6.6.0",
		"electron": "^17.1.0",
		"electron-builder": "^22.14.13",
		"electron-reload": "^2.0.0-alpha.1",
		"esbuild-loader": "^2.18.0",
		"eslint": "^8.9.0",
		"eslint-plugin-react": "^7.28.0",
		"html-webpack-plugin": "^5.5.0",
		"iconv-lite": "^0.6.3",
		"lodash-es": "^4.17.21",
		"lowdb": "^3.0.0",
		"rc-progress": "^3.2.4",
		"react": "^17.0.2",
		"react-addons-shallow-compare": "^15.6.3",
		"react-contextmenu": "^2.14.0",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^17.0.2",
		"react-flip-toolkit": "^7.0.13",
		"react-icons": "^4.3.1",
		"react-scrollbars-custom": "^4.0.27",
		"react-spinners": "^0.11.0",
		"sass": "^1.49.8",
		"sass-loader": "^12.6.0",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.2.6",
		"ts-node": "^10.5.0",
		"typescript": "^4.6.2",
		"webpack": "^5.69.1",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"@electron/remote": "^2.0.4",
		"electron-acrylic-window": "^0.5.11",
		"electron-store": "^8.0.1"
	}
}
