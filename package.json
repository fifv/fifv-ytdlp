{
	"name": "try-ytdlp",
	"packageManager": "yarn@3.1.1",
	"version": "1.6.4",
	"main": "build/main.js",
	"author": "Fifv",
	"description": "A GUI to use yt-dlp",
	"scripts": {
		"start": "electron .",
		"watch": "webpack --mode=development --watch --devtool=inline-source-map",
		"build": "webpack --mode=production",
		"electron make": "electron-forge make"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"ignore": [
					"src",
					".git",
					"playground",
					".*\\.web.*"
				]
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "try-ytdlp"
					}
				}
			]
		}
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.61",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.61",
		"@types/lodash-es": "^4.17.5",
		"@types/node": "^17.0.8",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "latest",
		"@typescript-eslint/parser": "latest",
		"copy-webpack-plugin": "^10.2.0",
		"css-loader": "^6.5.1",
		"electron": "^16.0.5",
		"electron-reload": "^2.0.0-alpha.1",
		"eslint": "^8.6.0",
		"eslint-plugin-react": "latest",
		"html-webpack-plugin": "^5.5.0",
		"sass-loader": "^12.4.0",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.2.6",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1"
	},
	"dependencies": {
		"@electron/remote": "^2.0.1",
		"bootstrap": "^5.1.3",
		"electron-acrylic-window": "^0.5.11",
		"electron-store": "^8.0.1",
		"encoding": "^0.1.13",
		"iconv-lite": "^0.6.3",
		"json5": "^2.2.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"sass": "^1.46.0",
		"typescript": "^4.5.4"
	}
}
