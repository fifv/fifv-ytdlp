{
    "name": "try-ytdlp",
    "version": "3.1.0-beta.1",
    "main": "build/main.js",
    "author": "Fifv",
    "description": "A GUI to use yt-dlp",
    "scripts": {
        "dev vite": "vite & nodemon --watch ./src/main.ts  --exec electron -r esbuild-register ./src/main.ts",
        "dist esbuild": "node ./esbuild.cjs electron && electron-builder",
        "build esbuild": "node ./esbuild.cjs electron sourcemap",
        "pack electron-builder": "taskkill /im \"YT-dlp GUI.exe\" /F & electron-builder",
        "analyze": "source-map-explorer 'build/**/*.js' --no-border-checks",
        "start": "electron .",
        "//dev esbuild": "node ./esbuild.cjs electron dev & nodemon --watch ./src/main.ts  --exec electron -r esbuild-register ./src/main.ts",
        "//webpack-watch": "webpack --mode=development --watch --devtool=source-map",
        "//webpack-dist": "webpack --mode=production && electron-builder",
        "//webpack build": "webpack --mode=production",
        "//electron-forge make": "electron-forge make"
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "ignore": [
                    "src",
                    ".git",
                    "playground",
                    ".*\\.web.*",
                    "dist"
                ]
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "yt-dlp-gui",
                        "noMsi": true
                    }
                }
            ]
        }
    },
    "devDependencies": {
        "@craftamap/esbuild-plugin-html": "^0.6.1",
        "@electron-forge/cli": "^6.3.0",
        "@electron-forge/maker-squirrel": "^6.3.0",
        "@electron/remote": "^2.0.10",
        "@swc/core": "^1.3.77",
        "@tippyjs/react": "^4.2.6",
        "@types/lodash-es": "^4.17.8",
        "@types/node": "^20.5.0",
        "@types/react": "^18.2.20",
        "@types/react-addons-shallow-compare": "^0.14.22",
        "@types/react-custom-scrollbars": "^4.0.10",
        "@types/react-dom": "^18.2.7",
        "@types/workerpool": "^6.4.0",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "@vitejs/plugin-react-swc": "^3.3.2",
        "add": "^2.0.6",
        "autoprefixer": "^10.4.15",
        "bootstrap": "^5.3.1",
        "builtin-modules": "^3.3.0",
        "clsx": "^2.0.0",
        "colorette": "^2.0.20",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "electron": "26.0.0",
        "electron-builder": "^24.6.3",
        "electron-reload": "^2.0.0-alpha.1",
        "electron-store": "^8.1.0",
        "esbuild": "^0.19.2",
        "esbuild-loader": "^4.0.0",
        "esbuild-plugin-clean": "^1.0.1",
        "esbuild-plugin-copy": "^2.1.1",
        "esbuild-register": "^3.4.2",
        "esbuild-sass-plugin": "^2.12.0",
        "esbuild-style-plugin": "^1.6.2",
        "eslint": "^8.47.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-react": "^7.33.2",
        "esno": "^0.17.0",
        "html-webpack-plugin": "^5.5.3",
        "iconv-lite": "^0.6.3",
        "lodash-es": "^4.17.21",
        "lowdb": "npm:@fifv/lowdb@^6.0.1",
