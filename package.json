{
	"name": "try-ytdlp",
	"packageManager": "yarn@3.1.1",
	"version": "2.11.0",
	"main": "build/main.js",
	"author": "Fifv",
	"description": "A GUI to use yt-dlp",
	"scripts": {
		"start": "electron .",
		"esbuild-watch": "node ./esbuild.js development",
		"esbuild-dist": "node ./esbuild.js production && electron-builder",
		"esbuild-prod": "node ./esbuild.js production sourcemap",
		"analyze": "source-map-explorer 'build/**/*.js' --no-border-checks",
		"electron-builder": "electron-builder",
		"webpack-watch": "webpack --mode=development --watch --devtool=source-map",
		"webpack-dist": "webpack --mode=production && electron-builder",
		"webpack build": "webpack --mode=production",
		"electron-forge make": "electron-forge make"
	},
	"config": {
		"forge": {
			"packagerConfig": {
				"ignore": [
					"src",
					".git",
					"playground",
					".*\\.web.*",
					"dist"
				]
			},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "yt-dlp-gui",
						"noMsi": true
					}
				}
			]
		}
	},
	"devDependencies": {
		"@craftamap/esbuild-plugin-html": "^0.4.0",
		"@electron-forge/cli": "^6.0.1",
		"@electron-forge/maker-squirrel": "^6.0.1",
		"@electron/remote": "^2.0.8",
		"@swc/core": "^1.3.16",
		"@tippyjs/react": "^4.2.6",
		"@types/lodash-es": "^4.17.6",
		"@types/node": "^17.0.45",
		"@types/react": "^18.0.25",
		"@types/react-addons-shallow-compare": "^0.14.22",
		"@types/react-custom-scrollbars": "^4.0.10",
		"@types/react-dom": "^18.0.8",
		"@types/workerpool": "^6.1.0",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1",
		"add": "^2.0.6",
		"bootstrap": "^5.2.2",
		"colorette": "^2.0.19",
		"copy-webpack-plugin": "^10.2.4",
		"css-loader": "^6.7.2",
		"electron": "^22.0.0-beta.5",
		"electron-builder": "^23.6.0",
		"electron-reload": "^2.0.0-alpha.1",
		"electron-store": "^8.1.0",
		"esbuild": "^0.14.54",
		"esbuild-loader": "^2.20.0",
		"esbuild-plugin-clean": "^0.9.0",
		"esbuild-plugin-copy": "^1.6.0",
		"esbuild-sass-plugin": "^2.4.0",
		"eslint": "^8.27.0",
		"eslint-plugin-react": "^7.31.10",
		"html-webpack-plugin": "^5.5.0",
		"iconv-lite": "^0.6.3",
		"lodash-es": "^4.17.21",
		"lowdb": "^3.0.0",
		"rc-progress": "^3.4.1",
		"react": "^18.2.0",
		"react-addons-shallow-compare": "^15.6.3",
		"react-contextmenu": "^2.14.0",
		"react-custom-scrollbars": "^4.2.1",
		"react-dom": "^18.2.0",
		"react-flip-toolkit": "^7.0.17",
		"react-icons": "^4.6.0",
		"react-scrollbars-custom": "^4.1.1",
		"react-spinners": "^0.13.6",
		"rimraf": "^3.0.2",
		"sass": "^1.56.1",
		"sass-loader": "^13.2.0",
		"source-map-explorer": "^2.5.3",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0",
		"workerpool": "^6.3.1",
		"yarn": "^1.22.19"
	},
	"dependencies": {
		"electron-acrylic-window": "^0.5.11"
	}
}
