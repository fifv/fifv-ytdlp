        "postcss": "^8.4.28",
        "rc-progress": "^3.4.2",
        "react": "^18.2.0",
        "react-addons-shallow-compare": "^15.6.3",
        "react-contextmenu": "^2.14.0",
        "react-custom-scrollbars": "^4.2.1",
        "react-dom": "^18.2.0",
        "react-flip-toolkit": "^7.1.0",
        "react-icons": "^4.10.1",
        "react-scrollbars-custom": "^4.1.1",
        "react-spinners": "^0.13.8",
        "rimraf": "^5.0.1",
        "sass": "^1.65.1",
        "sass-loader": "^13.3.2",
        "source-map-explorer": "^2.5.3",
        "style-loader": "^3.3.3",
        "tailwindcss": "^3.3.3",
        "tippy.js": "^6.3.7",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
